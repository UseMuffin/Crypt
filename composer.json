{
    "name": "muffin/crypt",
    "description": "",
    "type": "cakephp-plugin",
    "keywords": [
        "cakephp",
        "muffin",
        "crypt", "cryptography",
        "encrypt", "encryption",
        "decrypt",
        "cipher"
    ],
    "homepage": "https://github.com/usemuffin/crypt",
    "license": "MIT",
    "authors": [
        {
            "name": "Jad Bitar",
            "homepage": "http://jadb.io",
            "role": "Author"
        },
        {
            "name": "ADmad",
            "homepage": "https://github.com/ADmad",
            "role": "Author"
        },
        {
            "name": "Others",
            "homepage": "https://github.com/usemuffin/crypt/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/usemuffin/crypt/issues",
        "source": "https://github.com/usemuffin/crypt"
    },
    "require": {
        "ext-openssl": "*"
    },
    "require-dev": {
        "cakephp/cakephp": "~3.0",
        "cakephp/cakephp-codesniffer": "2.*",
        "phpunit/phpunit": "4.1.*"
    },
    "autoload": {
